<div class="form-group">
<%= form_for @branch do |f| %>
  <%= f.label :branch_name, :class=>"form-control col-md-2" %>
  <%= f.text_field :branch_name, :required => true, :autofocus => true, :class=>"form-control col-md-4" %>
  <br>
  <%= f.label :location, :class=>"form-control col-md-2" %>
  <%= f.text_field :location, :class=>"form-control col-md-4" %>
  <br>
  <%= f.label :email, :class=>"form-control col-md-2" %>
  <%= f.text_field :email, :class=>"form-control col-md-4" %>
  <br>
  <%= f.label :contact, :class=>"form-control col-md-2" %>
  <%= f.text_field :contact, :class=>"form-control col-md-4" %>
  <br>
  <%= f.label :user_id, :class=>"form-control col-md-2" %>
  <%= f.text_field :user_id, :class=>"form-control col-md-4" %>
  <br>
  <%= f.submit class: "btn btn-primary btn-lg"  %>

<% end %>
</div>
